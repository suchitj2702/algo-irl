{
  "version": 2,
  "functions": {
    "app/api/execute-code/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "app/api/execute-code/status/[id]/route.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "app/api/problem/prepare/route.ts": {
      "memory": 512,
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/api/(problem/prepare|execute-code|execute-code/status/.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://algoirl.ai,https://www.algoirl.ai,https://algo-irl.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Timestamp, X-Signature, X-Request-Id, X-Hp-Field, X-Client-Fingerprint"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ],
  "env": {
    "CODE_EXECUTION_TIMEOUT": "25000",
    "CODE_EXECUTION_MEMORY_LIMIT": "128",
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/problem/prepare",
      "destination": "/api/problem/prepare"
    },
    {
      "source": "/api/execute-code",
      "destination": "/api/execute-code"
    },
    {
      "source": "/api/execute-code/status/:submissionId",
      "destination": "/api/execute-code/status/:submissionId"
    }
  ]
}
